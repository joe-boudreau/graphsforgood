1.  get_registered_graph(username, graphID) -> graph_config
2.  get_all_graphs_for_user(username) -> graph_config[]

4.  move_temp_data_to_permanent(username, graphID, s3_filename)
5.  update_data(s3_filename, new data)
6.  edit_graph(username, graphID, graph_config)
7.  delete_graph(username, graphID)
8.  delete_user_and_graphs(username)



9. generate_new_graph   (done)